
var variable = "this is a test ";

public def call(va : string) {

	return Script.func(va);
}

extend Script:

public def func(value : string) {

	var test : array = [10, 12, 154];
	
	value = value + " " + test[2];
	
	return String.addString(value);
}

extend String:

public def addString(value : string) {
	return variable + value;
}











